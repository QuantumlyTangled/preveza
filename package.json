{
  "name": "preveza",
  "version": "0.0.1",
  "description": "Backend for docks",
  "author": "Docks Exchange Project",
  "maintainers": [
    "Charalampos Fanoulis <yo@fanoulis.dev>"
  ],
  "private": true,
  "license": "AGPL-v3",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write src test .github",
    "format:test": "prettier src test .github",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-fastify": "^7.4.4",
    "@nestjs/typeorm": "^7.1.3",
    "@sapphire/utilities": "^1.0.0",
    "fastify-helmet": "^5.0.1",
    "fastify-sensible": "^3.0.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.0.0",
    "@skyra/eslint-config": "^5.3.3",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.13",
    "@types/node": "^14.10.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.1",
    "eslint": "7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.4.2",
    "jest-circus": "^26.4.2",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner"
  }
}
